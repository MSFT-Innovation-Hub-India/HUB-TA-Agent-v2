# Entra Tenant ID associated with your Azure subscription
TENANT_ID="<the Entra Tenant ID for your Azure subscription>"

# Entra Tenant ID of the MSIT Corp Subscription associated with Microsoft Teams you use for work
HOST_TENANT_ID="<the Host tenant ID when running as a shared app>"

# Entra Application (Client) ID and Secret of the TAB application
CLIENT_ID="<the Client ID for your Azure application>"
CLIENT_SECRET="<the Client Secret for your Azure application>"

# Azure OpenAI Configuration
az_openai_endpoint="https://<youraoai>.openai.azure.com/"
az_deployment_name="gpt-4o"
az_openai_api_version="2025-01-01-preview"

# List of HUB cities where HUB Assistants are deployed
hub_cities="Atlanta, Boston, Chicago, Dallas, Detroit, Houston, Irvine, Minneapolis, New York, Philadelphia, Seattle, Silicon Valley, St. Louis, Toronto, Washington, Mexico City, Sao Paulo, Amsterdam, Brussels, Copenhagen, Dubai, Herzliya, Istanbul, London, Johannesburg, Milan, Munich, Oslo, Paris, Stockholm, Warsaw, Zurich, Beijing, Bengaluru, Seoul, Shanghai, Singapore, Sydney, Taipei, Tokyo"
# az_assistant_id="asst_Ik8QTgaUmK7PI2MyWPE68Gtc" # No longer needed - using Responses API instead of Assistants API

# The File_ID of the Agenda Word Document Template fthat is uploaded to the Data Store in Azure OpenAI, mapped to the Hub Location
# The Code Interpreter of Responses API uses this file to generate the Word document containing the Agenda for the Customer Engagement

hub_assistant_file_ids={"bengaluru": "assistant-CM47Ev6uB3u5G4T3wCtMYW", "mumbai": "assistant-XXXXXXXXXXXXXXXXXXXX", "delhi": "assistant-YYYYYYYYYYYYYYYYYYYY", "chennai": "assistant-ZZZZZZZZZZZZZZZZZZZZ", "hyderabad": "assistant-AAAAAAAAAAAAAAAAAAAAA"}

# Fallback file_ids if hub location is not mapped
file_ids="assistant-CM47Ev6uB3u5G4T3wCtMYW"

# Azure Blob Storage Configuration
az_blob_storage_account_name="<storage acount name for TAB>"
az_storage_rg="agent-ta-buddy-rg"
az_subscription_id="AZURE SUBSCRIPTION ID"

# The container where the prepared Agenda Documents will be stored
az_blob_container_name="agenda-docs"

# The container where the Speaker names for different topics, and the Hub Architects in that hub location are stored
az_blob_container_name_hubmaster="hub-master"

# The container that stores high level state information for the agent per user session
az_blob_container_name_state="tab-state"

# The container that stores golden documents for use in Agenda creation
az_blob_golden_docs_container_name="golden-repo"

# Log level to be used by the application
log_level="DEBUG"

# The Application Insights connection string for monitoring and logging
az_application_insights_key="CONNECTION STRING FOR APPLICATION INSIGHTS"