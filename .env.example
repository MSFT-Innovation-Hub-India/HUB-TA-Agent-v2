# Microsoft 365 Configuration
TENANT_ID=your_tenant_id_here
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here

# Azure OpenAI Configuration
az_openai_endpoint=https://your-openai-endpoint.openai.azure.com/
az_deployment_name=your_deployment_name
az_api_type=azure
az_openai_api_version=2024-02-15-preview

# Hub Cities (comma separated)
hub_cities=Bengaluru,Mumbai,Delhi,Chennai,Hyderabad

# Azure Application Insights
az_application_insights_key=your_application_insights_connection_string

# Logging Configuration
log_level=INFO

# Azure Assistant Configuration
az_assistant_id=your_azure_assistant_id

# Hub-specific Assistant File IDs (JSON format)
# This maps normalized hub names to their corresponding file IDs
hub_assistant_file_ids={"bengaluru": "assistant-CM47Ev6uB3u5G4T3wCtMYW", "mumbai": "assistant-XXXXXXXXXXXXXXXXXXXX", "delhi": "assistant-YYYYYYYYYYYYYYYYYYYY", "chennai": "assistant-ZZZZZZZZZZZZZZZZZZZZ", "hyderabad": "assistant-AAAAAAAAAAAAAAAAAAAAA"}

# Azure Blob Storage Configuration
az_blob_storage_account_name=your_storage_account_name
az_blob_container_name=your_container_name
az_blob_container_name_hubmaster=your_hubmaster_container_name
az_blob_container_name_state=your_state_container_name
az_storage_rg=your_storage_resource_group
az_subscription_id=your_azure_subscription_id

# Azure Key Vault Configuration (for reference only)
akv=your_key_vault_name